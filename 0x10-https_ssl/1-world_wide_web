#!/usr/bin/env bash
# subdomins

function(){
    commands=$(dig $1.$2 | grep -A1 'ANSWER SECTION:')
    echo $var | awk '{print "The subdomain '$1' is a "$4" record and points to "$5}'
}

if [ $2 ]
then
